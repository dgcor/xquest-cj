    "loop": true,
    "anchor": "none",
    "position": [0, 140],
    "size": [320, 40]
  },
  "event": {
    "id": "scrollTextUpdate",
    "action": {
      "name": "if.equal",
      "param1": "%game.hasDrawable.enemiesScroll%",
      "param2": false,
      "else": [
        { "name": "text.setText", "id": "txtEnemyName" },
        { "name": "text.setText", "id": "txtEnemyPoints" },
        {
          "name": "if",
          "params": ["%enemiesScroll.scrollPosition%", ">", 4400, "and", "%enemiesScroll.scrollPosition%", "<", 4580],
          "then": [
            { "name": "text.setText", "id": "txtEnemyName", "text": "Grunger" },
            { "name": "text.setText", "id": "txtEnemyPoints", "text": "200" }
          ]
        },
        {
          "name": "if",
          "params": ["%enemiesScroll.scrollPosition%", ">", 4080, "and", "%enemiesScroll.scrollPosition%", "<", 4260],
